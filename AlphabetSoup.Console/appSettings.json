{
  "SoupOptions": {
    "CultureCode": "es-es",
    "Size": 20,
    "NumWords": 10
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\Soup.log",
          "rollingInterval": "Year"
        }
      }
    ]
  }
}
